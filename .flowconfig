[ignore]

[include]

[libs]
flow-typed/
flow/libdefs/

[options]
module.ignore_non_literal_requires=true
esproposal.decorators=ignore
module.name_mapper.extension='styl' -> '<PROJECT_ROOT>/flow/CSSModule.js.flow'
module.name_mapper.extension='geojson' -> '<PROJECT_ROOT>/flow/GeoJSON.js.flow'
module.name_mapper='^app\/\(.*\)' -> '<PROJECT_ROOT>/src/app/\1'
module.name_mapper='^client\/\(.*\)' -> '<PROJECT_ROOT>/src/client/\1'
module.name_mapper='^server\/\(.*\)' -> '<PROJECT_ROOT>/src/server/\1'

