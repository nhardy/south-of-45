    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "geojson2svg": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.4",
    "nodemailer": "^4.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "https://registry.npmjs.org/prop-types/-/prop-types-15.5.8.tgz",
    "react": "^15.5.4",
    "react-chartjs-2": "^2.5.5",
    "react-display-name": "^0.2.0",
    "react-dom": "^15.5.4",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.0.0-beta",
    "react-redux": "^5.0.4",
