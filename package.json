{
  "name": "web-scaffold",
  "version": "0.0.0",
  "description": "An example web scaffold",
  "main": "server.js",
  "scripts": {
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm test -- --reporter min",
    "lint": "eslint ./src ./test ./build",
    "lint:fix": "eslint --fix ./src ./test ./build",
    "flow": "flow",
    "flowtypes": "flow-typed install --overwrite",
    "dev": "node ./build/index.js dev",
    "prod": "node ./build/index.js prod",
    "start": "node server.js",
    "test": "mocha",
    "package": "node ./build/index.js package",
    "deploy": "node ./build/index.js deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhardy/web-scaffold.git"
  },
  "author": "Nathan Hardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhardy/web-scaffold/issues"
  },
  "homepage": "https://github.com/nhardy/web-scaffold#readme",
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "build",
      "coverage",
      "dist",
      "flow",
      "flow-typed",
      "node_modules",
      "test",
      "server.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "all": true
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "archiver": "^1.3.0",
    "autoprefixer": "^6.7.7",
    "azur": "^0.0.2",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "buffer-loader": "0.0.1",
    "cache-loader": "^1.0.3",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "del": "^2.2.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.18.0",
    "eslint-config-nhardy": "^0.3.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^3.0.0-rc.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.45.0",
    "flow-typed": "^2.1.2",
    "glob": "^7.1.2",
    "httpolyglot": "^0.1.2",
    "jsdom": "^10.0.0",
