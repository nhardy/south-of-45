    "proxyquire": "^1.7.11",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-svg-loader": "^1.1.1",
    "run-sequence": "^1.2.2",
    "selfsigned": "^1.9.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svgo-loader": "^1.2.1",
    "text-loader": "^0.0.1",
    "thread-loader": "^1.0.3",
    "url-loader": "^0.5.8",
